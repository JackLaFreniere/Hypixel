{
  "headers": [
    {
      "source": "**/*.+(css|js|png|jpg|jpeg|gif|svg|webp|json)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "**/*.html",
      "headers": [
        {
          "key": "Cache-Control", 
          "value": "public, max-age=3600, must-revalidate"
        }
      ]
    }
  ]
}